package com.pixelboob.lufa.lib{	import com.pixelboob.lufa.Interpreter;	import org.spicefactory.lib.logging.LogContext;	import org.spicefactory.lib.logging.Logger;	import flash.utils.Dictionary;			/**	 * @author mikej	 */	[LufaClass(className="os")]	public class OS 	{		private static var logger : Logger = LogContext.getLogger(OS);		[LufaMethod(methodName="date")]		public static function date(args : Array) : void		{			for each(var s:String in args)			{				trace("arg: " + s);			}		}		[LufaMethod(methodName="difftime", returnValues=1)]		public static function difftime(args : Array) : Array		{			return [(args[0] as Number) - (args[1] as Number)];		}		[LufaMethod(methodName="time", returnValues=1)]		public static function time(args : Array) : Array		{			var date : Date;						/*			 * year, month, and day, and may have fields hour, min, sec, and isdst			 */			if(args[0])			{				var defaults : Dictionary = args[0] as Dictionary;								date = new Date(defaults["year"], defaults["month"], defaults["day"]);								if(defaults["hour"])				{					date.setHours(defaults["hour"]);				}								if(defaults["min"])				{					date.setMinutes(defaults["min"]);				}								if(defaults["sec"])				{					date.setSeconds(defaults["sec"]);				}								if(defaults["isdst"])				{					// TODO!					logger.warn("Daylight saving time cannot be manually set yet");				}			}			else			{				date = new Date();			}						return [Math.round(date.getTime()/1000)];		}	}}