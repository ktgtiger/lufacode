package com.pixelboob.lufa.lib {	import flash.utils.Dictionary;			/**	 * @author mikej	 */	public class Table 	{				[LufaMethod(methodName="rawget")]		public static function rawget(args:Array):Array		{			var table : Dictionary = (args[0] as Dictionary);			var index : Object = args[1];						return [table[index]];		}				[LufaMethod(methodName="rawset")]		public static function rawset(args:Array):void		{			var table : Dictionary = (args[0] as Dictionary);			var index : Object = args[1];			var value : Object = args[2];						table[index] = value;		}				[LufaMethod(methodName="setmetatable")]		public static function setmetatable(args:Array):void		{			var table : Dictionary = (args[0] as Dictionary);			var metatable : Dictionary = (args[1] as Dictionary);						table["__metatable"] = metatable;		}				[LufaMethod(methodName="getmetatable")]		public static function getmetatable(args:Array):Array		{			var object : Object = (args[0]);			if(object is Dictionary)			{				return [(object as Dictionary)["__metatable"]];			}			else			{				return null;			}		}						// TODO : Move me somewhere sane ;)		[LufaMethod(methodName="print")]		public static function print(args:Array):void		{			var out : Array = [];			for each(var arg : Object in args)			{				if(arg is Dictionary)				{					// TODO : Typically this would print the address of the dictionary, which we don't have ;)					out.push("table: "+(arg as Dictionary));				}				else if(arg is String || arg is Boolean || arg is Number)				{					out.push(arg);				}				else				{					out.push(arg);				}			}						trace(out.join("\t"));		}			}}