package com.pixelboob.lufa.lib{	/**	 * @author mikej	 */	 	[LufaClass(className="io")]	public class IO 	{		[LufaMethod(methodName="write")]		public static function write(args:Array):void		{			trace("Here with "+args.length+" args");			var pieces : Array = [];			for each(var arg : Object in args)			{				if(arg == null)				{					pieces.push("nil");				}				else				{					pieces.push(arg);				}			}			trace(pieces.join(","));		}	}}