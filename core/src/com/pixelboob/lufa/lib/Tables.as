package com.pixelboob.lufa.lib {	import com.pixelboob.lufa.Interpreter;	import com.pixelboob.lufa.Table;			/**	 * @author mikej	 */	[LufaClass(className="table")]	public class Tables 	{		public static var interpreter : Interpreter;				[LufaMethod(methodName="foreach")]		public static function foreach(args:Array) : void		{			var table : Table = args[0] as Table;			var funcRef : Object = args[1] as Object;						for(var key : Object in table)			{				trace("key: "+key);				interpreter.callInternalRef(funcRef, key, table[key]);			}		}	}}