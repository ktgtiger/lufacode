package com.pixelboob.lufa {	import org.spicefactory.lib.logging.LogContext;	import org.spicefactory.lib.logging.Logger;		import flash.utils.Dictionary;		/**	 * @author mikej	 */	public dynamic class Table extends Dictionary 	{		private var logger : Logger = LogContext.getLogger(Table);				public function Table(weakKeys : Boolean = false)		{			super(weakKeys);		}				public function get length() : uint		{			var i : uint = 0;						if(this[i] == null)			{				return 0;			}						while(this[i+1] != null)			{				i++;			}						return i;		}	}}